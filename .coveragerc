[run]
source = src
omit = 
    */tests/*
    */test_*
    */venv/*
    */.venv/*
    */migrations/*
    */settings/*
    */manage.py
    */wsgi.py
    */asgi.py
    */__pycache__/*
    */node_modules/*
    */htmlcov/*
    */.git/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov

[xml]
output = coverage.xml 